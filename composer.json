{
    "name": "sarahman/laravel-timestamp-boolean-fields",
    "description": "Deal with the fields whose timestamp value means their boolean status.",
    "keywords": ["timestamp", "boolean", "fields", "property", "framework", "laravel", "lumen"],
    "type": "library",
    "version": "1.1.2",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Syed Abidur Rahman",
            "email": "aabid048@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "illuminate/database": "^4.0",
        "illuminate/events": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Sarahman\\Database\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    }
}
